int readPin=A5;
void setup() {
  // put your setup code here, to run once:
pinMode(readPin,INPUT);
Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
analogRead(readPin);
Serial.println(analogRead(readPin));
delay(500);
}
//This Code reads a potentiometer's value in 10bit (0-1023)
